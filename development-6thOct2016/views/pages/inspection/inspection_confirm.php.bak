                    </div>
                
                    <div class="oncorColumn2"> 
                        <rn:widget path="input/FormInput" name="incidents.c$inspection_type" label_input="Inspection Type" required="true"/>
                    </div>                    
                
                <div class="oncorColumnClear"></div>
            </div>      
                        <rn:widget path="input/FormInput" name="incidents.c$street_addr" required="true" />
                        	
                    <div class="oncorColumn1">
                        <rn:widget path="input/FormInput" name="incidents.c$city" label_input="City" required="true"/>
                    </div>
               
                <div class="oncorColumnClear"></div>    	

